{
  "compilerOptions": {
    "target": "es5", 
    "lib": [
      "es5",
      "es6",
      "dom"
    ], 
    "module": "umd", 
    "declaration": true, 
    "outDir": "./dist", 
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true
  }
}